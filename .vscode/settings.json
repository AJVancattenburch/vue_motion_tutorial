      },
      {
        "scope": "entity.name.type.rainbow8",
        "settings": {
          "foreground": "#008080",
          "fontStyle": "underline"
        }
      },
      {
        "scope": "markup.bold.rainbow9",
        "settings": {
          "foreground": "#F58231"
